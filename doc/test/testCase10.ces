Procedure test() is
	z: Integer;
	w: Integer;
	function decrementa (x:Integer) return Integer is
		y:Integer;
		begin
			y := x-1;
			return y;
	end decrementa;
		

begin
	Put_line("***INVOCACION DE SUBPROGRAMAS***");
	z:=3;
	w := decrementa(z);
	Put_line("w(2)=");
	Put_line(w);
	Put_line("FIN);
	
end test